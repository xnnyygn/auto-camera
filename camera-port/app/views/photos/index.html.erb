<h1>Listing photos</h1>

<table>
  <thead>
    <tr>
      <th>Aws key</th>
      <th>Width</th>
      <th>Height</th>
      <th>Content Type</th>
      <th>File Size</th>
      <th>Date token</th>
      <th>Location</th>
      <th>Camera</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= link_to photo.aws_key, show_content_photo_path(photo) %></td>
        <td><%= photo.width %></td>
        <td><%= photo.height %></td>
        <td><%= photo.content_type %></td>
        <td><%= number_to_human_size(photo.file_size) %></td>
        <td><%= photo.date_token %></td>
        <td><%= photo.location %></td>
        <td><%= photo.camera.name %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
